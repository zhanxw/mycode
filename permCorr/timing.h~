#ifndef _TIMING_H_
#define _TIMING_H_

#include <time.h>
#include "debug.h"

static time_t start, end;

extern void timing_start(void) {
    time(&start);
    PTIME(start);
}

extern void timing_stop(void) {
    time(&end);
    PTIME(end);
}

extern void timing_diff
#endif /* _TIMING_H_ */
